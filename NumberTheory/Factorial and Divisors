Note 
the number of divisors of a number is very very small 

vectorll generate_divisors(ll n)     O( sqrt(n) )
{
    vector<ll> v;
    ll i;
    for(i = 1 ; i*i < n  ;++i)
        if(n%i == 0)
            v.push_back(i), v.push_back(n/i);
 
    if(i*i == n)
        v.push_back(i);
 
    return v;    Not sorted
}
----------------------------------------------------------------------------------------------------------------------------------------
// wilson's theroem 
   
   -(p-1)! % p = p-1   IFF p is prime
   -the only prime factorial is 2!
----------------------------------------------------------------------------------------------------------------------------------------
Factorial % mod
1/factorial % mod
fact[0]=1;
for(int i=1;i<=3000000;i++)
   fact[i]=(fact[i-1]*i)%mod;
   
Ifact[3000000]=power(fact[3000000],mod-2);
for(ll i=3000000-1;i>=0;i--)
   Ifact[i]=(Ifact[i+1]*(i+1))%mod;
